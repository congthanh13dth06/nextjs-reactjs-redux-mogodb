  "main": "./src/app.server.js",
  "scripts": {
    "dev": " nodemon ./src/app.server.js --exec babel-node --presets @babel/preset-env --watch ./src/app.server.js",
    "build": "npm run build:next && npm run build:server && npm run build:folder",
